{
    "name": "ODrive Micro X1",
    "mcu": "STM32H72x",
    "min_voltage": 6.0,
    "max_voltage": 30.0,
    "inverters": [
        {"drv": {"$ref": "drv8316"}, "max_current": 15.0}
    ],
    "rails": {},
    "aux_inverters": [],
    "io": {
        "gpio0": "io",
        "gpio1": "t",
        "gpio2": "io",
        "gpio3": "io",
        "gpio4": "io",
        "gpio5": "ia",
        "gpio6": "ia",
        "gpio7": "i",
        "gpio8": "io",
        "gpio9": "io",
        "gpio10": "iop",
        "gpio11": "io",
        "miso": "",
        "mosi": "",
        "sck": ""
    },
    "inc_enc": [
        {"a": "gpio2", "b": "gpio3"}
    ],
    "spi": [
        {"miso": "miso", "mosi": "mosi", "sck": "sck"}
    ],
    "uart": [
        {"rx": "gpio9", "tx": "gpio8"}
    ],
    "step_dir": [
        {"step": "gpio10", "dir": "gpio11"}
    ],
    "rs485": [],
    "temp_in": [{"io": "gpio1", "r_load": 1000, "thermistor_bottom": true}],
    "fan": false,
    "onboard_encoders": 1,
    "defaults": {
        "inc_enc_z": ["gpio4"],
        "spi_ncs": ["gpio0"]
    }
}