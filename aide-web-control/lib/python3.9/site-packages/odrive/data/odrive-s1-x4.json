{
    "name": "ODrive S1 X4",
    "mcu": "STM32H72x",
    "min_voltage": 10.5,
    "max_voltage": 54.0,
    "inverters": [
        {"drv": {"$ref": "drv8353"}, "max_current": 150.0}
    ],
    "rails": {
        "5v": 5.0,
        "vcc": 3.3
    },
    "aux_inverters": [{}],
    "io": {
        "gpio0": "iod",
        "gpio1": "ioa",
        "gpio2": "io",
        "gpio3": "io",
        "gpio4": "t",
        "gpio5": "i",
        "gpio6": "o",
        "gpio7": "i",
        "gpio8": "ip",
        "gpio9": "io",
        "gpio10": "io",
        "gpio11": "ioa",
        "gpio12": "o",
        "miso": "",
        "mosi": "",
        "sck": ""
    },
    "inc_enc": [
        {"a": "gpio3", "b": "gpio9"}
    ],
    "spi": [
        {"miso": "miso", "mosi": "mosi", "sck": "sck"}
    ],
    "uart": [
        {"rx": "gpio7", "tx": "gpio6"}
    ],
    "step_dir": [
        {"step": "gpio8", "dir": "gpio5"}
    ],
    "rs485": ["gpio0"],
    "temp_in": [{"io": "gpio4", "r_load": 1000, "thermistor_bottom": true}],
    "fan": false,
    "onboard_encoders": 1,
    "defaults": {
        "inc_enc_z": ["gpio10"],
        "spi_ncs": ["gpio12"]
    }
}